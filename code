import java.util.Scanner;

public class MCQQuiz {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        String[][] questions = {
            {"What is the capital of France?", "A. Paris", "B. London", "C. Berlin", "D. Madrid", "A"},
            {"What is the highest mountain in the world?", "A. Mount Kilimanjaro", "B. Mount Everest", "C. Mount McKinley", "D. Mount Fuji", "B"},
            {"What is the largest animal in the world?", "A. Blue whale", "B. Elephant", "C. Giraffe", "D. Gorilla", "A"}
        };
        
        int score = 0;
        for (int i = 0; i < questions.length; i++) {
            String[] question = questions[i];
            System.out.println(question[0]);
            for (int j = 1; j < question.length - 1; j++) {
                System.out.println(question[j]);
            }
            String answer = scanner.nextLine().toUpperCase();
            if (answer.equals(question[question.length - 1])) {
                System.out.println("Correct!");
                score++;
            } else {
                System.out.println("Incorrect. The correct answer is " + question[question.length - 1]);
            }
            System.out.println();
        }
        
        System.out.println("Your final score is " + score + " out of " + questions.length);
    }
}
